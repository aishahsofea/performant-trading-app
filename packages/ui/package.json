{
  "name": "@repo/ui",
  "version": "0.0.0",
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./src/styles.css",
