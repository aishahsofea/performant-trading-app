{
  "name": "@repo/dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:report": "lighthouse http://localhost:3000/ --output=json --output-path=./performance-report.json",
    "lighthouse:report-budget": "lighthouse http://localhost:3000/ --budget-path=./scripts/performance-budget.json",
    "analyze-bundle": "cross-env ANALYZE=true next build",
    "performance:check-budgets": "ts-node src/scripts/performanceBudgetCheck.js"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@tailwindcss/postcss": "^4.1.11",
    "@vercel/analytics": "^1.5.0",
    "cross-env": "^10.0.0",
    "next": "15.4.5",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "typescript": "latest"
  }
}
